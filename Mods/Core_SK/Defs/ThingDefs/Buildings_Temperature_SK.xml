	<ThingDef ParentName="BuildingBase">
		<defName>Building_Heater</defName>
		<label>Coal Burner</label>
		<thingClass>SK_Heater.Building_Heater</thingClass>
		<graphicData>
		<texPath>Things/Building/heater</texPath>
		<graphicClass>Graphic_Single</graphicClass>
		<drawSize>(2,2)</drawSize>
		</graphicData>
		<altitudeLayer>Waist</altitudeLayer>
		<passability>PassThroughOnly</passability>
		<pathCost>70</pathCost>
		<fillPercent>1</fillPercent>
		<castEdgeShadows>true</castEdgeShadows>
		<statBases>
			<MaxHitPoints>310</MaxHitPoints>
			<WorkToMake>2500</WorkToMake>
			<Flammability>0</Flammability>
		</statBases>
		<tickerType>Normal</tickerType>
		<description>A coal burner produces only heat by burning basic coal-like fuel sources. It requires a hopper filled with coal, charcoal, kindling or peat in order to function.</description>
		<size>(2,2)</size>
		<building>
			<ignoreNeedsPower>true</ignoreNeedsPower>
			<wantsHopperAdjacent>true</wantsHopperAdjacent>
		</building>
			<stuffCategories>
				<li>Stony</li>
			</stuffCategories>
			<costStuffCount>100</costStuffCount>
    			<costList>
				<Spare_Parts>6</Spare_Parts>
    			</costList>
		<comps>
      			<li>
        				<compClass>SK_Heater.CompFeed</compClass>        
      			</li>
			<li>
				<compClass>SK_Heater.CompHeatPusherFeed</compClass>
				<heatPerSecond>50</heatPerSecond>
				<heatPushMaxTemperature>50</heatPushMaxTemperature>
			</li>
			<li>
				<compClass>CompGlower</compClass>
				<glowRadius>5</glowRadius>
				<glowColor>(217,112,33,0)</glowColor>
			</li>
		</comps>
		<terrainAffordanceNeeded>Light</terrainAffordanceNeeded>
		<designationCategory>Temperature</designationCategory>
		<staticSunShadowHeight>0.20</staticSunShadowHeight>
	</ThingDef>
	
	
	<ThingDef ParentName="BuildingBase">
		<defName>HeaterHopper</defName>
		<label>Coal Burner Hopper</label>
		<thingClass>Building_Storage</thingClass>
		<graphicData>
		<texPath>Things/Building/Production/Hopper</texPath>
		<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<altitudeLayer>Waist</altitudeLayer>
		<passability>PassThroughOnly</passability>
		<fillPercent>0.5</fillPercent>
		<pathCost>70</pathCost>
		<building>
      <fixedStorageSettings>
        <priority>Important</priority>
        <filter>
          <thingDefs>
            <li>Kindling</li>
            <li>Coal</li>
            <li>Peat</li>
			<li>Charcoal</li>
          </thingDefs>
        </filter>
      </fixedStorageSettings>
      <defaultStorageSettings>
        <priority>Important</priority>
        <filter>
          <thingDefs>
            <li>Kindling</li>
            <li>Coal</li>
            <li>Peat</li>
			<li>Charcoal</li>
          </thingDefs>
        </filter>
      </defaultStorageSettings>
		</building>
		<inspectorTabs>
			<li>ITab_Storage</li>
		</inspectorTabs>
		<castEdgeShadows>true</castEdgeShadows>
		<statBases>
			<WorkToMake>450</WorkToMake>
			<MaxHitPoints>100</MaxHitPoints>
			<Flammability>1.0</Flammability>
		</statBases>
		<description>Holds resources (coal, charcoal, peat, kindling) for the Coal Burner.</description>
	<stuffCategories>
	<li>Stony</li>
	</stuffCategories>
	<costStuffCount>10</costStuffCount>
		<tickerType>Never</tickerType>
		<rotatable>true</rotatable>
		<neverMultiSelect>true</neverMultiSelect>
		<designationCategory>Temperature</designationCategory>
		<staticSunShadowHeight>0.3</staticSunShadowHeight>
		<surfaceType>Item</surfaceType>
		<placeWorkers>
			<li>PlaceWorker_NextToHopperAccepter</li>
		</placeWorkers>
	</ThingDef>



