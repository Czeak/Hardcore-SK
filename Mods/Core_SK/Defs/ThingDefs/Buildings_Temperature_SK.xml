  <ThingDef ParentName="BuildingBase">
    <defName>Campfire</defName>
    <label>Campfire</label>
    <description>A camfire can cook meals and produce heat but it will burn out after a few days. As with all heat sources, it must be placed indoors so it has a closed space to heat.</description>    
    <thingClass>Building_Campfire</thingClass>
    <category>Building</category>
    <graphicData>
      <texPath>Things/Building/Misc/Campfire</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawRotated>false</drawRotated>
    </graphicData>
    <altitudeLayer>Waist</altitudeLayer>
    <passability>PassThroughOnly</passability>
    <pathCost>50</pathCost>
    <constructEffect>ConstructDirt</constructEffect>
    <tickerType>Normal</tickerType>
    <drawerType>RealtimeOnly</drawerType>
    <statBases>
      <MaxHitPoints>80</MaxHitPoints>
      <WorkToMake>200</WorkToMake>
      <Flammability>0</Flammability>
    </statBases>
    <selectable>true</selectable>
		<costList>
			<Kindling>15</Kindling>
		</costList>
    <soundImpactDefault>BulletImpactGround</soundImpactDefault>
    <placeWorkers>
      <li>PlaceWorker_Heater</li>
    </placeWorkers>
    <drawPlaceWorkersWhileSelected>true</drawPlaceWorkersWhileSelected>
    <comps>
      <li>
        <compClass>CompGlower</compClass>
        <glowRadius>10</glowRadius>
        <glowColor>(252,187,113,0)</glowColor>
      </li>
      <li>
        <compClass>CompHeatPusher</compClass>
        <heatPerSecond>15</heatPerSecond>
        <heatPushMaxTemperature>28</heatPushMaxTemperature>
      </li>
      <li>
        <compClass>CompGatherSpot</compClass>
      </li>
      <li>
        <compClass>CompLifespan</compClass>
        <lifespanTicks>40000</lifespanTicks>
      </li>
    </comps>
    <designationCategory>Temperature</designationCategory>
    <designationHotKey>I</designationHotKey>
    <hasInteractionCell>True</hasInteractionCell>
    <interactionCellOffset>(0,0,-1)</interactionCellOffset>
    <recipes>
      <li>CookMealJerky</li>
	  <li>CookRoastedMeat</li>
    </recipes>
    <inspectorTabs>
      <li>ITab_Bills</li>
    </inspectorTabs>
    <leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
    <killedLeavings>
      <Ash>2</Ash>
    </killedLeavings>
    <building>
      <isMealSource>true</isMealSource>
      <spawnedConceptLearnOpportunity>BillsTab</spawnedConceptLearnOpportunity>
      <deconstructible>true</deconstructible>
    </building>
  </ThingDef>


