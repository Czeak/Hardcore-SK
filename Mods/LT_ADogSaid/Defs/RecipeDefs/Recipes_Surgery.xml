        <jobString>Installing Bionic Animal Eye.</jobString>
        <workAmount>2000</workAmount>
        <ingredients>
            <li>
                <filter>
                    <categories>
                        <li>Medicine</li>
                    </categories>
                </filter>
                <count>1</count>
            </li>
