<?xml version="1.0" encoding="utf-8" ?>
<ResearchProjectDefs>


			<ResearchProjectDef>
			    <defName>RTStorage_QuantumStockpile_SelfDefrag</defName>
			    <label>Quantum Stockpiles Self-defrag</label>
			    <description>Allows quantum stockpiles to periodically minimize the amount of stored incomplete stacks.</description>
			    <totalCost>1000</totalCost>
			    <prerequisites>
			      <li>StorageII</li>
			    </prerequisites>
			    <researchMods>
			      <li>
			        <specialAction>RTStorage.ResearchModsSpecial.QuantumStockpileUpgradeSelfDefrag</specialAction>
			      </li>
			    </researchMods>
			</ResearchProjectDef>

			<ResearchProjectDef>
			    <defName>RTStorage_QuantumStockpile_1</defName>
			    <label>Quantum Stockpiles Upgrade 1</label>
			    <description>Allows quantum stockpiles to store one additional stack of items for a total of five per cell.</description>
			    <totalCost>1000</totalCost>
			    <prerequisites>
			      <li>StorageII</li>
			    </prerequisites>
			    <researchMods>
			      <li>
			        <specialAction>RTStorage.ResearchModsSpecial.QuantumStockpileUpgrade1</specialAction>
			      </li>
			    </researchMods>
			  </ResearchProjectDef>

			  <ResearchProjectDef>
			    <defName>RTStorage_QuantumStockpile_2</defName>
			    <label>Quantum Stockpiles Upgrade 2</label>
			    <description>Quantum stockpiles store one more stack of items for a total of six per cell.</description>
			    <totalCost>1200</totalCost>
			    <prerequisites>
			      <li>RTStorage_QuantumStockpile_1</li>
			    </prerequisites>
			    <researchMods>
			      <li>
			        <specialAction>RTStorage.ResearchModsSpecial.QuantumStockpileUpgrade2</specialAction>
			      </li>
			    </researchMods>
			  </ResearchProjectDef>

			  <ResearchProjectDef>
			    <defName>RTStorage_QuantumStockpile_3</defName>
			    <label>Quantum Stockpiles Upgrade 3</label>
			    <description>Quantum stockpiles store one more stack of items for a total of seven per cell.</description>
			    <totalCost>1500</totalCost>
			    <prerequisites>
			      <li>RTStorage_QuantumStockpile_2</li>
			    </prerequisites>
			    <researchMods>
			      <li>
			        <specialAction>RTStorage.ResearchModsSpecial.QuantumStockpileUpgrade3</specialAction>
			      </li>
			    </researchMods>
			  </ResearchProjectDef>

			  <ResearchProjectDef>
			    <defName>RTStorage_QuantumStockpile_4</defName>
			    <label>Quantum Stockpiles Upgrade 4</label>
			    <description>Quantum stockpiles store one more stack of items for a total of eight per cell.</description>
			    <totalCost>2000</totalCost>
			    <prerequisites>
			      <li>RTStorage_QuantumStockpile_3</li>
			    </prerequisites>
			    <researchMods>
			      <li>
			        <specialAction>RTStorage.ResearchModsSpecial.QuantumStockpileUpgrade4</specialAction>
			      </li>
			    </researchMods>
			  </ResearchProjectDef>


</ResearchProjectDefs>